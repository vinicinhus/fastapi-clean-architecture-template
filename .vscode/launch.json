{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug FastAPI com Uvicorn",
        "type": "debugpy",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "app.main:app",
          "--reload",
          "--host",
          "127.0.0.1",
          "--port",
          "8000"
        ],
        "console": "integratedTerminal",
        "env": {
          "ENVIRONMENT": "development"
        },
        "jinja": true
      }
    ]
  }
  